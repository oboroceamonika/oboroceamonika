{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": {
      "text": "Human Development Index",
      "subtitle": ["P21 Countries' HDI, most recent year", "Source: UN"]
    },
    "description": "A simple bar chart with embedded data.",
    "data": {
      "url": "https://raw.githubusercontent.com/oboroceamonika/oboroceamonika.github.io/main/viz.csv"
    },
    "width": 600,
    "height": 300,
    "layer": [
      {
        "mark": "point",
        "encoding": {
          "x": {"aggregate": "mean", "field": "T_TRANS_UNIT"},
          "y": {"aggregate": "mean", "field": "UNEMP_RATE"},
          "detail": {"field": "DATE"}
        }
      },
      {
        "mark": {
          "type": "text",
          "dy": -10  
        },
        "encoding": {
          "x": {"aggregate": "mean", "field": "T_TRANS_UNIT"},
          "y": {"aggregate": "mean", "field": "UNEMP_RATE"},
          "text": {"field": "DATE"}
        }
      }
    ]
  }
   